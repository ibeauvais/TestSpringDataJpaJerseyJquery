



<div id="estabCreation" >
	<a id="estabCreationClose">x</a>  
	<form id="formEstabCreation"/>
		<p>
			<label>Nom :</label>
			<input type="text" name="name" value="${nom}" />
		</p>
		<p>
			<label>Adresse :</label>
			 <input type="text" name="address" value="${address}" />
		</p>
		<p>
			<label>code postal :</label>
			<input type="text" name="postalCode" value="${postalCode}" />
		</p>
		<p>
			<label>Ville:</label>
			 <input type="text" name="city" value="${city}" />
		</p>
		<p>
			<label>Téléphone :</label>
			<input type="text" name="phone" value="${phone}" />
		</p>
		<p>
			<label>Fax :</label>
			<input type="text" name="fax" value="${fax}" />
		</p>
		<p>
			<label>Description :</label>
			<textarea name="description">${description}</textarea>
		</p>
	</form>
	
	<input id="buttonCreerEstab" type="button" name="Valider" value="valider" />

</div>
 <div id="backgroundPopup"></div>  